

pipeline {
    agent any
    environment {
        DOCKER_REGISTRY = 'maryusmm'
        IMAGE_NAME      = 'simple-java-maven-app'
    }

    stages {

        stage('Deploy Container') {            

            steps{
                sh "echo "Docker image version $DOCKER_IMAGE_VERSION""
                sh "docker run -d $DOCKER_REGISTRY/$IMAGE_NAME:${DOCKER_IMAGE_VERSION}"
            }
        }


    }
}
